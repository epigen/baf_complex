    val <- rep(0,N)
    for (i in 1:N){
        val[i] <- tryCatch( sqrt( mean(((as.numeric(rARID1A.A[i,validV]) - as.numeric(rARID1A.B[i,validV]))^2),na.rm=T )) , error=function(e) {NA})
        #val[i] <- tryCatch(1-cor(as.numeric(rARID1A.A[i,validV]),as.numeric(rARID1A.B[i,validV]),use="complete.obs"), error=function(e) {NA})
        if (sum(is.na(as.numeric(rARID1A.A[i,validV]) + as.numeric(rARID1A.B[i,validV])))>17) val[i] = NA
    }
