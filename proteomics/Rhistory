data <- BRG1.ab[which(BRG1.ab[,8]=="BAF_complex"),38:41]
        N <- dim(data)[1]
        dataMean <- data
        dataSd <- data
        for(i in 1:N){
            dataMean[i] = tryCatch( mean(data[i,],na.rm=T),error = function(e) {0})
            dataSd[i] =   tryCatch( sd(data[i,],na.rm=T), error = function(e) {0})
        }
